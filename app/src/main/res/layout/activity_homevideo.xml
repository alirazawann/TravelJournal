<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homevideo">



    <ProgressBar
        android:id="@+id/progress_circle35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="156dp"
        android:layout_marginTop="177dp"
        android:layout_marginLeft="156dp"
        android:layout_alignParentLeft="true" />

    <TextView
        android:id="@+id/textView735"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="Home Videos"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="5dp" />


    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycler_view35"
        android:layout_width="match_parent"
        android:layout_height="423dp"
        android:layout_above="@+id/button455"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true">

    </android.support.v7.widget.RecyclerView>

    <Button
        android:id="@+id/button635"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:onClick="gotoprofile"
        android:text="Profile"
        android:layout_alignParentRight="true" />

    <Button
        android:id="@+id/button1435"
        android:layout_width="56dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button63"
        android:onClick="incityfunc"
        android:text="Button"
        android:layout_toLeftOf="@+id/button63" />


    <Button
        android:id="@+id/button1835"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button635"
        android:onClick="mainfunc"
        android:text="Button"
        android:layout_toLeftOf="@+id/button635" />

    <Button
        android:id="@+id/button195"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="gotoaddphoto"
        android:text="+" />

    <Button
        android:id="@+id/button455"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button635"
        android:layout_alignParentStart="true"
        android:onClick="hivideo"
        android:text="Images"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/button465"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/recycler_view35"
        android:text="Videos"
        android:layout_alignParentRight="true" />

    <Button
        android:id="@+id/button47"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/button1835"
        android:onClick="wishlistfunc"
        android:text="Button"
        android:layout_toLeftOf="@+id/button1835" />

</RelativeLayout>